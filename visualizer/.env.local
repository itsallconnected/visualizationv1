# GitHub OAuth Configuration
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id
REACT_APP_GITHUB_REPO=username/ai-alignment-visualization
REACT_APP_GITHUB_BRANCH=main
REACT_APP_AUTH_REDIRECT_URI=http://localhost:3000/auth-callback
REACT_APP_OAUTH_PROXY_URL=https://your-oauth-proxy.example.com/authenticate

# Application Configuration
REACT_APP_BASE_URL=http://localhost:3000
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_ENCRYPTION=true
REACT_APP_ENABLE_VERSIONING=true
REACT_APP_ENABLE_MULTI_SPHERE=true
REACT_APP_ENABLE_ADMIN=true

# GitHub Data Paths
REACT_APP_COMPONENTS_PATH=ai-alignment/components.json
REACT_APP_SUBCOMPONENTS_PATH=ai-alignment/subcomponents.json
REACT_APP_SPHERES_PATH=data/spheres.json
REACT_APP_USERS_PATH=data/users 